<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.minealex2244.univtest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UnivTest">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SecondActivity"
            android:label="@string/second_activity_title"
            android:exported="true">
            <intent-filter>
                <action android:name="com.minealex2244.univtest.secondactivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ProdDetails"
            android:label="Details"
            android:exported="true">
            <intent-filter>
                <action android:name="com.minealex2244.univtest.proddetails" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".GalleryActivity"
            android:label="Thread Gallery"
            android:exported="true">
            <intent-filter>
                <action android:name="com.minealex2244.univtest.galleryactivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".GalleryAsyncActivity"
            android:label="Thread Async Gallery"
            android:exported="true">
            <intent-filter>
                <action android:name="com.minealex2244.univtest.galleryasyncactivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity2"
            android:label="Main"
            android:exported="true">
            <intent-filter>
                <action android:name="com.minealex2244.univtest.mainactivity2" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>